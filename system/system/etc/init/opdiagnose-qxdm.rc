
#ifdef VENDOR_EDIT
#Danny.Xiao@System, 2019/12/17, [AN-918] add for property-trigger mechanism
#To copy the QXDM-mask to the specific path before QXDM enabling.
on property:vendor.opdiagnose.qxdm.mask=modem_general
    mkdir /sdcard/oem_log
    mkdir /sdcard/oem_log/diag_logs
    copy /system/etc/mask/NEW_ALL.cfg /sdcard/oem_log/diag_logs/Diag.cfg
on property:vendor.opdiagnose.qxdm.mask=modem_voice
    mkdir /sdcard/oem_log
    mkdir /sdcard/oem_log/diag_logs
    copy /system/etc/mask/GSM_WCDMA_NAS_DS.cfg /sdcard/oem_log/diag_logs/Diag.cfg
